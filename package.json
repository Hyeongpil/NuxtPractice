{
  "name": "nuxt-starter-kit",
  "version": "1.0.0",
  "description": "",
  "author": "Devist",
  "private": true,
  "scripts": {
    "local": "npm run mockup &npm run storybook & cross-env NODE_ENV=local nodemon server/index.js --watch server --open",
    "dev": "npm run storybook & cross-env NODE_ENV=development nodemon server/index.js --watch server open",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "mockup": "cd server/mock && ts-node-dev --respawn mock-server.ts",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "nuxt-buefy": "^0.3.2",
    "nuxt-property-decorator": "^2.7.2",
    "nuxt-validate": "^1.0.1",
    "vee-validate": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/tailwindcss": "^1.4.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/vue": "^5.3.18",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "cors": "^2.8.5",
    "cypress": "^4.5.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.9",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "storybook-readme": "^5.0.8",
    "tailwindcss": "^1.4.6",
    "ts-loader": "^7.0.5",
    "ts-node-dev": "^1.0.0-pre.44",
    "vuex-module-decorators": "^0.17.0"
  }
